<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class="row">
  <div class="col-md-4">
    <ul class="users">
      <%= render  @users %>
    </ul>
  </div>
</div>

<div class="pagination">
  <div class="pagination_info">
    <%= page_entries_info @users %>
  </div>
  <%= will_paginate @users, renderer: BootstrapPagination::Rails,
                            container: false %>
</div>
