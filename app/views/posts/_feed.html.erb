<ul class="posts">
  <% posts.each do |post| %>
    <%= render partial: 'posts/headline_info', locals: { post: post } %>
  <% end %>
</ul>

<div class="pagination">
  <div class="pagination_info">
    <%= page_entries_info posts, model: 'post' %>
  </div>
  <%= will_paginate posts, renderer: BootstrapPagination::Rails,
                           container: false %>
</div>
